<resource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/rest-api-3.xsd"
          name="tutorial"
          displayName="Tutorial API"
          version="1.0">
    <resource name="create">
        <method type="post">
            <service name="tutorial.TutorialServices.createTutorial"/>
        </method>
    </resource>
    
    <resource name="get">
        <method type="get">
            <service name="tutorial.TutorialServices.getTutorial"/>
        </method>
    </resource>

</resource>
